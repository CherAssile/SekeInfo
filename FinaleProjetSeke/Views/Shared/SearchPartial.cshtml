@model IEnumerable<FinaleProjetSeke.Utilisateur>
@{
    ViewBag.Title = "PartialView";
}


@if (Model != null)
{
    foreach (var item in Model)
    {
        <div class="col-xl-4 col-md-6">
            <div class="product">
                <div class="product_image"><img src="../images/(@item.Photo)" alt="nopic" style="weight:260px;height:260px;"></div>
                <div class="product_content">
                    <div class="product_info d-flex flex-row align-items-start justify-content-start">
                        <div>
                            <div>
                                <div class="product_name">@(item.Nom)</div>

                                <div class="product_price text-right">@(item.Email)</div>

                            </div>
                        </div>
                        <div class="ml-auto text-right">
                            <div class="rating_r rating_r_4 home_item_rating"><i></i><i></i><i></i><i></i><i></i></div>
                            <div class="product_price text-right">@(item.Code)</div>
                        </div>
                    </div>
                    <div class="product_buttons">
                        <div class="text-right d-flex flex-row align-items-start justify-content-start">
                            <div class="product_button product_fav text-center d-flex flex-column align-items-center justify-content-center">
                                <div><div><img src="../images/dollar-sign.svg" class="svg" alt="">Voir en ecran</div></div>
                            </div>
                            <div class="product_button product_cart text-center d-flex flex-column align-items-center justify-content-center">

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    }
}
else
{
    <center> jguyguy </center>
}